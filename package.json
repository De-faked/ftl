{
  "name": "fos7a-taibah-institute",
  "version": "1.0.0",
  "description": "Website for Fos7a Taibah Institute",
  "author": "Fos7a Taibah Institute",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fos7a-taibah-website"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"App.tsx\" \"index.tsx\" --report-unused-disable-directives --max-warnings 0",
    "i18n:check": "rg -n --glob \"*.tsx\" --glob \"*.ts\" \"(aria-label=\\\"[^\\\"]+\\\"|title=\\\"[^\\\"]+\\\"|placeholder=\\\"[^\\\"]+\\\"|>[^<{]*[A-Za-z][^<{]*<)\" components src App.tsx",
    "preview": "vite preview",
    "i18n:audit": "python3 scripts/i18n-audit-ui-strings.py",
    "photos:build": "node scripts/build-photos.mjs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.48.0",
    "aws4fetch": "^1.0.20",
    "axios": "^1.6.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "postcss": "^8.4.38",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^8.50.0",
    "vite": "5.4.2"
  }
}
